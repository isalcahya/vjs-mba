{
  "name": "vjs-mba",
  "version": "1.0.0",
  "description": "An extension and skin for video.js used for mba site",
  "main": "dist/vjs-mba.umd.js",
  "module": "dist/vjs-mba.es5.js",
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "dev": "export NODE_ENV=dev && rollup -c -w",
    "build": "npm run prebuild && rollup -c rollup.config.js",
    "prebuild": "rimraf dist",
    "server:dev": "npm run server:development && node server",
    "server:development": "mix",
    "server:watch": "mix watch",
    "server:watch-poll": "mix watch -- --watch-options-poll=1000"
  },
  "author": "isal.c",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@videojs/http-streaming": "^2.14.0",
    "express": "^4.17.3",
    "filename-regex": "^2.0.1",
    "laravel-mix": "^6.0.43",
    "lodash": "^4.17.21",
    "mix-html-builder": "^0.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "sass": "^1.32.8",
    "video.js": "^7.18.1",
    "videojs-contrib-quality-levels": "^2.1.0",
    "videojs-plus-font": "Pong420/font#af50d0e"
  }
}
